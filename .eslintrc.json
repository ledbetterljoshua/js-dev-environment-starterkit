{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true,
      "impliedStrict": true
    },
    "sourceType": "module"
  },
  "rules": {
    "eqeqeq": [
      "error"
    ],
    "no-var": [
      "warn"
    ],
    "no-duplicate-imports": [
      "error"
    ],
    "prefer-const": [
      "error"
    ],
    "prefer-spread": [
      "error"
    ],
    "no-console": [
      "off"
    ]
  }
}
